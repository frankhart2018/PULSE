var mass_block = 15
var coeff_static_friction = 0.18
var accel_of_trolley = 0.5
var force_exerted = mass_block * accel_of_trolley
print("The force on the block caused by the motion of trolley = ", force_exerted)
print(" N")
var force_static_ex = mass_block * coeff_static_friction * 10
print("Force of static friction between the block and the trolley = ", force_static_ex)
print(" N")
