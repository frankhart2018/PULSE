var third_term_AP = 4
var ninth_term_AP = -8
var first_term_a
var common_difference_d
var term_euals_zero
common_difference_d = (third_term_AP-ninth_term_AP)/-6
print("Common difference of AP:")
print(common_difference_d)
first_term_a = third_term_AP - 2*common_difference_d
print("First term of AP:")
print(first_term_a)
term_euals_zero =-first_term_a/common_difference_d
print("The term of AP that equals zero is:")
print(term_euals_zero+1)
